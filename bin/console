#!/usr/bin/env ruby

lib = File.expand_path("../lib", __dir__)
$LOAD_PATH.unshift(lib) unless $LOAD_PATH.include?(lib)

require "bundler/setup"
require "pry"

require "journey"
require "row_parser"
require "csv"

JOURNEYS =
  CSV
    .foreach("data/full.csv", headers: true)
    .map { |row| RowParser.new(row).parse }

Pry.start
